{
    "atomKeymap.promptV3Features": true,
    "editor.fontFamily": "JetBrains Mono",
    "editor.multiCursorModifier": "ctrlCmd",
    "editor.formatOnPaste": true,
    "editor.cursorStyle": "line",
    "editor.cursorWidth": 4,
    "editor.renderControlCharacters": true,
    "editor.rulers": [80, 88, 100],
    "editor.suggestSelection": "first",
    "editor.wordWrapColumn": 80,
    "workbench.list.horizontalScrolling": false,
    /*
    "workbench.colorCustomizations": {
        "editorCursor.foreground": "#ff22aa",
        "editorCursor.background": "#ff22aa",
        "editorBracketMatch.border": "#ff22aa",
        "editorBracketMatch.background": "#ff22aa22"
    },
    */
    "files.autoSave": "off",
    "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
    "files.exclude": {
        "**/.classpath": false,
        "**/.project": false,
        "**/.settings": false,
        "**/.factorypath": false,
        "**/.git": false
    },
    "java.semanticHighlighting.enabled": true,
    "python.languageServer": "Pylance",
    "rust-client.channel": "stable",
    "go.formatTool": "goimports",
    "C_Cpp.updateChannel": "Insiders",
    "diffEditor.codeLens": true,
    "cmake.configureOnOpen": true,
    "terminal.integrated.fontFamily": "JetBrains Mono ExtraBold",
    "rust-client.engine": "rls",
    "rust-analyzer.cargo.allFeatures": true,
    "rust-analyzer.inlayHints.enable": false,
    "workbench.editorAssociations": {
        "*.ipynb": "jupyter-notebook"
    },
    "kite.showWelcomeNotificationOnStartup": false,
    "[go]": {
        "editor.insertSpaces": false,
        "editor.formatOnSave": false,
        "editor.codeActionsOnSave": {
            "source.organizeImports": true
        }
    },
    "files.watcherExclude": {
        "**/.bloop": true,
        "**/.metals": true,
        "**/.ammonite": true
    },
    "mssql.connections": [
        {
            "server": "{{put-server-name-here}}",
            "database": "{{put-database-name-here}}",
            "user": "{{put-username-here}}",
            "password": "{{put-password-here}}"
        }
    ],
    "gitlens.defaultDateFormat": "Do MMMM YYYY @ H:mm:ssZ",
    "gitlens.defaultDateShortFormat": "D/M/YYYY H:mm:ss",
    "gitlens.defaultTimeFormat": "H:mm:ssZ",
    "[cpp]": {
        "editor.defaultFormatter": "ms-vscode.cpptools"
    },
    "glassit.alpha": 215,
    "terminal.integrated.tabs.enabled": true,
    "workbench.editor.untitled.hint": "hidden",
    "workbench.startupEditor": "welcomePage",
    "editor.fontLigatures": true,
    "codesnap.backgroundColor": "#ffffff00",
    "telemetry.telemetryLevel": "off",
    "files.associations": {
        "CMakeLists.txt": "cmake",
        "*.txt": "plaintext",
        "*.html": "html",
        "*.prolog": "prolog",
        "*.make": "makefile"
    },
    "java.project.importHint": false,
    "workbench.iconTheme": "helium-icon-theme",
    "workbench.productIconTheme": "fluent-icons",
    "codespaces.accountProvider": "Uninitialized",
    "editor.suggest.preview": true,
    "glassit.step": 8,
    "[html]": {
        "editor.tabSize": 2,
        "editor.suggest.insertMode": "replace",
        "gitlens.codeLens.scopes": [
            "document"
        ]
    },
    "editor.wordWrap": "on",
    "better-comments.tags": [
        {
            "tag": "!",
            "color": "#FF2D00",
            "strikethrough": false,
            "underline": false,
            "backgroundColor": "transparent",
            "bold": false,
            "italic": false
        },
        {
            "tag": "?",
            "color": "#3498DB",
            "strikethrough": false,
            "underline": false,
            "backgroundColor": "transparent",
            "bold": false,
            "italic": false
        },
        {
            "tag": "//",
            "color": "#474747",
            "strikethrough": false,
            "underline": false,
            "backgroundColor": "transparent",
            "bold": false,
            "italic": false
        },
        {
            "tag": "todo",
            "color": "#FF8C00",
            "strikethrough": false,
            "underline": false,
            "backgroundColor": "transparent",
            "bold": false,
            "italic": false
        },
        {
            "tag": "*",
            "color": "#98C379",
            "strikethrough": false,
            "underline": false,
            "backgroundColor": "transparent",
            "bold": false,
            "italic": false
        }
    ],
    "editor.inlineSuggest.enabled": true,
    "redhat.telemetry.enabled": false,
    /*
    "java.configuration.runtimes": [
        {
            "name": "JavaSE-1.8",
            "path": "/home/renoir/.sdkman/candidates/java/8.0.312-tem",
        },
        {
            "name": "JavaSE-17",
            "path": "/home/renoir/.sdkman/candidates/java/17.0.1-tem",
        }
    ],
    */
    "notebook.cellToolbarLocation": {
        "default": "right",
        "jupyter-notebook": "left"
    },
    "[python]": {
        "gitlens.codeLens.symbolScopes": [
            "!Module"
        ],
        "editor.wordBasedSuggestions": false
    },
    "workbench.colorTheme": "Liver Dark Full",
    "editor.fontSize": 16,
    "workbench.enableExperiments": false,
    "workbench.settings.enableNaturalLanguageSearch": false,
    "update.mode": "manual",
}
